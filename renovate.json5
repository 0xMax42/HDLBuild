{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "https://git.0xmax42.io/renovate-bot/presets/raw/tag/v0/base.json5",
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "enabled": true,
      "automerge": false
    },
    {
      "matchUpdateTypes": [
        "major"
      ],
      "enabled": false
    }
  ]
}